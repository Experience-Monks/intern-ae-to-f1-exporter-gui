{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "consistent-return": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "import/no-unresolved": [2, { ignore: ['electron'] }],
    "react/jsx-no-bind": 0,
    "react/jsx-indent": 0,
    "indent": 0,
    "keyword-spacing": 0,
    "react/prefer-stateless-function": 0,
    "no-trailing-spaces": 0,
    "object-curly-spacing": 0,
    "brace-style": 0,
    "prefer-const": 0,
    "jsx-quotes": 0,
    "arrow-body-style": 0,
    "react/jsx-indent-props": 0,
    "global-require": 0,
    "no-else-return": 0,
    "no-params-reassign": 0,
    "padded-blocks": 0
  },
  "plugins": [
    "import",
    "react"
  ],
  "settings": {
    "import/resolver": "webpack"
  }
}
